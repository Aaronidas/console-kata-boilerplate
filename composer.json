{
  "name": "aaronidas/console-kata-boilerplate",
  "description":  "Boilerplate for a kata in php with symfony console",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Aarón Bernabeu Rodríguez",
      "email": "aaronidasbr@gmail.com"
